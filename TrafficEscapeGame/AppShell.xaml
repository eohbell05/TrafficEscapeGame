<Shell
    x:Class="TrafficEscapeGame.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TrafficEscapeGame">

    <ShellContent Route="MainPage"
                  ContentTemplate="{DataTemplate local:MainPage}" />

    <ShellContent Route="GamePage"
                  ContentTemplate="{DataTemplate local:GamePage}" />

    <ShellContent Route="SettingsPage"
                  ContentTemplate="{DataTemplate local:SettingsPage}" />
</Shell>
